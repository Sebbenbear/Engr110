
/**
 * Write a description of class Item here.
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
public class Item
{
    private int id;
    private int region;
    private boolean status;
    private String name;

    /**
     * Constructor for objects of class Item
     */
    public Item(int id, int region, boolean status, String name)
    {
        this.id = id;
        this.region = region;
        this.status = status;
        this.name = name;
    }

    /**
     * 
     */
    public int getID()
    {
        return id;
    }
    
    public String getName()
    {
        return name;
    }
    
    public int getRegion()
    {
        return region;
    }
    
    
}
